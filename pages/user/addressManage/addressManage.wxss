/* page/user/addressManage/addressManage.wxss */
page{
  background: #F4F4F4;
}

.address-list{
  background: #fff;
  width: 750rpx;
  height: 157rpx;
  border-bottom: 1rpx solid #f4f4f4;
}
.address-content{
  width: 590rpx;
  height: 94rpx;
  float: left;
  margin-top: 40rpx;

  border-right:1rpx solid #f4f4f4;
  text-indent: 20rpx;
}

.editSign{
  width: 36rpx;
  height: 36rpx;
  margin: 0 auto;
  background-image:url("data:image/png;base64,") ;
  background-repeat: no-repeat;
  background-size: 100%;
}

.homeSign{
  width: 52rpx;
  height:52rpx;
  background-image:url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 100%; 
  margin-left: 26rpx;
}

.eduInstitutionSign{
  width:81rpx;
  height: 55rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 100%;
  margin-left: 26rpx;
}


.parentSign{
  width: 102rpx;
  height: 55rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 100%;
  margin-left: 26rpx;
}

.companySign{
  width: 80rpx;
  height: 52rpx;
  background-image: url("data:image/png;base64,");
   background-repeat: no-repeat;
  background-size: 100%;
  margin-left: 26rpx;
}

.edit{
  width: 158rpx;
  height: 94rpx;
  float: left;
  margin-top: 40rpx;
  text-align: center;
  font-size:28rpx;
  font-family:Source Han Sans CN;
  font-weight:400;
  color:#333;
}


.add-addressBtn{
    font-family: SourceHanSansCN-Medium;
    font-size: 36rpx;
    font-weight: bold;
    color:#fff;
    width: 710rpx;
    height: 90rpx;
    line-height: 90rpx;
    border-radius: 45rpx;
    text-align: center;
    margin: 0 auto;
    background: #F6931F;
    margin-top: 21rpx;
}
.hasno_area{
    display: flex;
    justify-content: center;
    flex-direction: column;
    flex-wrap: wrap;
    align-items: center;
    height : calc(100vh - 94px);
}
.hasno_area image{
    width : 314px;
}
.info{
    text-align : center;
    font-size:16px;
    font-family:Source Han Sans CN;
    font-weight:400;
    color:rgba(158,158,158,1);
}
.fix_height{
    height : 231.5px;
}

.default{
  display: inline-block;
	font-size: 28rpx;
	color: #f2826a;
	border: #f2826a solid 1px;
	text-indent: 0;
	padding-left: 10rpx;
	padding-right: 10rpx;
	height: 48rpx;
	line-height: 48rpx;
}