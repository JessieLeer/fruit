/* page/user/addressManage/newAddress/newAddress.wxss */
page{
  background: #f3f3f3;
}
.add-addressBtn{
    font-family: SourceHanSansCN-Medium;
    font-size: 36rpx;
    font-weight: bold;
    color:#fff;
    width: 710rpx;
    height: 90rpx;
    line-height: 90rpx;
    border-radius: 45rpx;
    text-align: center;
    margin: 0 auto;
    background: #F6931F;
    margin-top: 21rpx;
}

.box{
  width: 712rpx;
  margin: 0 auto;
  height: 110rpx;
  border-bottom:1px solid #f3f3f3;

}
.titleName{
  width: 250rpx;
  height: 110rpx;
  line-height: 110rpx;
  text-align: left;
  text-indent: 18rpx;
  font-size:32rpx;
  font-family:Source Han Sans CN;
  font-weight:400;
  color:#686868;
  float: left;
}
.inputStyle{
  height: 110rpx;
  width:500rpx; 
  margin-left: 207rpx;
  font-size:32rpx;
  font-family:Source Han Sans CN;
  font-weight:400;
  color:#333;
}

.selSign{
  height: 110rpx;
  width:520rpx; 
  margin-left: 207rpx;
}
.selSign>view{
  float: right;
  height:52rpx;
  text-align:center;
  line-height:52rpx;
  background:#F8F8F8;
  border:1rpx solid #e0e0e0;
  border-radius:3px;
  margin-right: 40rpx;
  margin-top: 32rpx;
  font-size:28rpx;
  font-family:Source Han Sans CN;
  font-weight:400;
  color:#565656;
}

.sel{
  background:#F6931F !important;
  border:1rpx solid#F6931F  !important;
  border-radius:3px  !important;
  color:#fff  !important;
}


.sex{
  width:32rpx;
  height: 32rpx;
  border-radius: 50%;
  border: 1rpx solid #f3f3f3;
  float: left;
  margin-top: 5rpx;
}

.selSex{
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  float: left;
  margin-top: 5rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 100%;
}

.woman,.man{
  float: left;
  width: 336rpx;
  margin-top: 41rpx;
}
.woman{
  width: 308rpx;
}
.man{
  margin-left: 68rpx;
}
.woman>view{
  float: right;
}

.toLeft{
  width: 20rpx;
  height: 20rpx;
  border-top:1rpx solid #d3d3d3;
  border-right: 1rpx solid #d3d3d3;
  transform:rotate(45deg);
  position: absolute;
  top:45rpx;
  right:5rpx;
}
.popup{
    position: fixed;
    width : 100vw;
    height :100vh;
    background : rgba(0,0,0,.2);
    top : 0;
    left : 0;
    z-index : 9999999;
}
.autoheight{
    position: absolute;
    height : 100%;
    width: 100%;
}
.fix{
    position: absolute;
    bottom : 0;
    width  : 100%;
    left : 0;
    z-index : 9999999;
}
.van-picker__confirm{
    color :#676767!important;
}
.van-picker__cancel{
    color :#676767!important;
}