/* page/user/setting/phone-set/phone-set.wxss */
.next-step{
  font-family: SourceHanSansCN-Regular;
  text-align: center;
  font-weight: 400;
  width: 359rpx;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 36rpx;
  color: #fff;
  background: #F6931F;
  margin: 0 auto;
  border-radius: 45rpx;
  margin-top:60rpx;
}

.title-style{
  float: left;
  width: 134rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color:#333;
  font-size:32rpx;
  font-weight: 400;
}
.input-style{
  width: 597rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: left;
  border: none;
  border-bottom:2rpx solid #dfdfdf;
  font-size: 32rpx;
}


.btn-getCode{
  position: absolute;
  right:20rpx;
  top:10rpx;
  width: 196rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background: #fff;
  border:1rpx solid #F6931F;
  font-size: 26rpx;
  color:#F6931F;
}
button[disabled] {
    color: #333333 !important;
    background: #F4F4F4 !important;
    border-color: #D9D9D9 !important;
}
.safa-sign{
  width: 141rpx;
  height: 141rpx;
  margin: 0 auto;
  margin-top: 80rpx;
  margin-bottom: 55rpx;
  background-image:url('data:image/png;base64,');

  background-size: 100%;
}
.header-area{
  width:100%;
  height:164rpx;
  background:rgba(246,246,246,1);
  margin-bottom : 133rpx;
  display: flex;
}
.header-area view{
  width : 50%;
  text-align : center;
  line-height : 164rpx;
  font-size:32rpx;
  font-family:Source Han Sans CN;
  font-weight:400;
  color:rgba(111,111,111,1);
}
.header-area view:nth-child(1){
  width : 50%;
  text-align : center;
  line-height : 164rpx;
  font-size:38rpx;
  font-family:Source Han Sans CN;
  font-weight:400;
  color:rgba(246,147,31,1);
}
.tip image{
  width : 43rpx;
  height: auto;
  vertical-align: -4px;
  margin-right : 17rpx;
}
.tip{

  padding : 216rpx 21rpx;
}
.title_info{
  font-size:32rpx;
font-family:Source Han Sans CN;
font-weight:bold;
color:rgba(51,51,51,1);
}
.info_area{
  font-size:30rpx;
  font-family:Source Han Sans CN;
  font-weight:400;
  color:rgba(130,130,130,1);
  margin-top : 31rpx;
}