page{
  background-color: #f4f4f4;
}

.active.van-tab--active {
  color: #F6931F;
}

.good-wrapper{
	overflow: auto;
}

.good{
  margin-top: 14rpx;
}

.good-header.van-cell:after{
  border-bottom: none;
}

.good-header .van-cell__title{
  color: #A5A5A5;
}

.good-header .van-cell__value{
  color: #F6931F;
}

.good-content{
  padding-left: 30rpx;
	padding-right: 30rpx;
	padding-top: 30rpx;
	padding-bottom: 20rpx;
}

.image-wrapper{
	width: 100%;
	white-space: nowrap; 
}

.good-image{
  width: 120rpx;
	margin-right: 30rpx;
	display: inline-block;
}

.good-count{
  text-align: right;
	padding-left: 30rpx;
	padding-top: 20rpx;
	font-size: 28rpx;
	letter-spacing: 4rpx;
}

.good-count-count{
  color: #A5A5A5;
	margin-right: 10rpx;
}

.good-footer{
  text-align: right;
}

.cancel.van-button--default {
  background: rgba(255,255,255,1);
  border: 1px solid #D7D7D7;
	color: #6a6a6a;
}

.pay.van-button--warning{
  background:rgba(246,147,31,1);
	border: 1px solid #F6931F;
	color: white;
}

.cancel.van-button--default,.pay.van-button--warning{
  border-radius:45px;
	margin-left: 30rpx;
} 

.more-text{
  text-align: center;
	color: #AFAFAF;
	font-size: 24rpx;
	display: block;
	padding-top: 20rpx;
	padding-bottom: 20rpx;
}

.count-time{
  margin-top: 14rpx;
	border: #60BAB4 solid 1px;
	margin-left: 20rpx;
	margin-right: 20rpx;
	border-radius: 6rpx;
}

.canvas-code{
  background-color: #fff;
	width: 200px;
	height: 200px;
	margin: 0 auto;
}

.group-header{
  display: flex;
	height: 44px;
	line-height: 44px;
	padding-left: 30rpx;
	padding-right: 30rpx;
}

.group-header-left{
  flex: 1;
}

.group-time{
  color: #A5A5A5;
	font-size: 14px;
}

.group-header-right{
  width: 100rpx;
	color: #F6931F;
	font-size: 14px;
	text-align: right;
}

.group-user.van-tag{
  margin-left: 30rpx;
	background:rgba(96,186,180,1);
  border-radius:22px;
	font-size: 20rpx;
}

.nogood{
  text-align: center;
	padding-top: 100rpx;
}

.nogood-text{
  display: block;
	color: #9e9e9e;
	font-size: 32rpx;
}