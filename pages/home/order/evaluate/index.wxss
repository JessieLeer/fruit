page{
  background-color: #f9f9f9;
}

.rate-area{
	background-color: white;
	padding-top: 100rpx;
	padding-bottom: 100rpx;
	margin-bottom: 14rpx;
}

.rate-question{
  color: #656565;
	display: block;
	margin-bottom: 26rpx;
}

.f-tac{
  text-align: center;
}

.rate-content{
  background-color: white;
	padding-bottom: 30rpx;
}

.rate-message{
  height: 200rpx;
	overflow: auto;
}

.image-pick{
  width: 140rpx;
	height: 140rpx;
	border: dashed 2rpx #BFBFBF;
	color: #bfbfbf;
	font-size: 24rpx;
}

.photograph{
  margin-top: 20rpx;
}

.f-db{
  display: block;
}

.f-ib{
  display: inline-block;
}

.f-vat{
  vertical-align: top;
}

.rate-images{
  padding-left: 30rpx;
	padding-right: 30rpx;
}

.evaluate-image{
  width: 140rpx;
	height: 140rpx;
	margin-right: 30rpx;
	border: #BFBFBF solid 1px;
	position: relative;
}

.evaluate-image image{
  width: 100%;
	max-height: 140rpx;
}

.submit.van-button--warning{
  width: 90%;
	border-radius: 40rpx;
	background-color: #F6931F;
	margin: 120rpx auto 0 auto;
	display: block;
}

.del{
  position: absolute;
	z-index: 40;
	top: -10rpx;
	right: -10rpx;
}